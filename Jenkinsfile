pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building.. mxu 2019'
                sh 'date'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                sh 'date'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                sh 'date'
                sh 'oc --help'
                sh 'date '
            }
        }
    }
}
